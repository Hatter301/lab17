#include <iostream>
#include <vector>
using namespace std;
int main() {
	int n, a, i = 0, j, s = 1;
	vector <int> v;
	vector <int> b;
	vector <int> c;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		for (i = 0; i < n; i++) {
			cin >> a;
			v.push_back(a);
		}
		i = 0;
		for (i; i < n - 1; i++)
			if (v[i] != v[i + 1]) {
				b.push_back(1);
				c.push_back(v[i]);
			}
			else {
				for (j = i; v[j] == v[j + 1]; j++) {
					if (j == n - 1)
						break;
					s++;
				}
				b.push_back(s);
				c.push_back(v[i]);
				s = 1;
				i = j;
			}
		if (v[n - 1] != v[n - 2]) {
			b.push_back(1);
			c.push_back(v[n-1]);
		}
		for (i = 0; i < b.size(); i++)
			cout << b[i] << " ";
		cout << endl;
		for (i = 0; i < c.size(); i++)
			cout << c[i] << " ";
	}
	return 0;
}
