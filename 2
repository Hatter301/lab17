#include <iostream>
#include <vector>
using namespace std;
int main() {
	int n, a, i, j=0, s = 1, l;
	vector <int> v;
	cout << "Введите L" << endl;
	cin >> l;
	cout << "Введите N" << endl;
	cin >> n;
	if ((n <= 0) || (l <= 0))
		cout << "Данные введены некорректно";
	else {
		cout << "Введите элементы массива" << endl;
		for (i = 0; i < n; i++) {
			cin >> a;
			v.push_back(a);
		}
		i = 0;
		for (i; i < (v.size()-1); i++) {
			if (v[i] == v[i + 1]) {
				for (j = i; ((v[j] == v[j + 1]) && (j < (v.size()-1))); j++)
					s++;
				if (s > l) {
					v.erase(v.begin() + i, v.begin() + (j+1));
					v.insert(v.begin() + i, 0);

				}
				else i = j;
				s = 1;
			}
		}
		for (i = 0; i < v.size(); i++)
			cout << v[i] << " ";
	}
	return 0;
}
